{
  "name": "frontend-seed-4",
  "version": "1.6.2",
  "description": "A basic HTML starter template for Bootstrap 4",
  "keywords": [
    "css",
    "sass",
    "html",
    "responsive",
    "theme",
    "template"
  ],
  "homepage": "https://github.com/trungnghia112/frontend-seed-4",
  "author": "trungnghia112 <trungnghia112@gmail.com>",
  "bugs": {
    "url": "https://github.com/trungnghia112/frontend-seed-4/issues",
    "email": "trungnghia112@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/trungnghia112/frontend-seed-4"
  },
  "scripts": {
    "start": "gulp serve",
    "prettier": "prettier ./**/*.{ts,json,md,scss} --write",
    "clean": "gulp clean",
    "build": "npm run clean && npm run prettier && gulp build",
    "release": "standard-version",
    "publish": "git push --follow-tags origin master && npm publish"
  },
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.10.3"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.24.6",
    "eslint": "^5.3.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^3.10.0",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.1",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "prettier": "1.14.2"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "jquery": "^3.2.1",
    "popper.js": "^1.14.4",
    "standard-version": "^4.4.0"
  }
}
